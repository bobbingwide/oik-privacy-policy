<phpunit
	bootstrap="../../plugins/oik-batch/oik-wp.php"
	backupGlobals="false"
	colors="false"
	convertErrorsToExceptions="true"
	convertNoticesToExceptions="true"
	convertWarningsToExceptions="true"
	>
	<!-- for PHPUnit 5.7, 6.1, 6.2 -->
	<testsuites>
		<testsuite name="oik-privacy-policy">
			<directory prefix="Tests_" suffix=".php">tests/</directory>
		</testsuite>
	</testsuites>
</phpunit>
